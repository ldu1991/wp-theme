{
    "version": 2,
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "settings": {
        "appearanceTools": true,
        "useRootPaddingAwareAlignments": true,
        "border": {
            "color": true,
            "radius": true,
            "style": true,
            "width": true
        },
        "color": {
            "background": true,
            "custom": true,
            "customDuotone": false,
            "customGradient": true,
            "defaultGradients": false,
            "defaultPalette": false,
            "duotone": [],
            "gradients": [
                {
                    "slug": "red-linear",
                    "gradient": "linear-gradient(90deg, #E10303 9.49%, #C70202 100%)",
                    "name": "Red Linear"
                }
            ],
            "link": true,
            "palette": [
                {
                    "slug": "white",
                    "color": "#FFFFFF",
                    "name": "White"
                },
                {
                    "slug": "black",
                    "color": "#000000",
                    "name": "Black"
                }
            ],
            "text": true
        },
        "custom": {
            "heading": {
                "typography": {
                    "fontWeight": 700,
                    "lineHeight": 1.25,
                    "fontSize": {
                        "h1": "clamp(2.5rem, 2.5rem + ((1vw - 0.36rem) * 5.3398), 5.25rem)",
                        "h2": "clamp(1.875rem, 1.875rem + ((1vw - 0.36rem) * 1.2136), 2.5rem)",
                        "h3": "1.5rem",
                        "h4": "1.125rem",
                        "h5": "1.125rem",
                        "h6": "1.125rem"
                    }
                },
                "margin": {
                    "top": "0",
                    "bottom": "30px"
                },
                "padding": {
                    "top": "20px"
                }
            },
            "paragraph": {
                "margin": {
                    "top": "0",
                    "bottom": "20px"
                }
            }
        },
        "layout": {
            "contentSize": "960px",
            "wideSize": "1400px"
        },
        "spacing": {
            "blockGap": true,
            "margin": true,
            "padding": true,
            "units": [
                "%",
                "px",
                "em",
                "rem",
                "vh",
                "vw"
            ]
        },
        "typography": {
            "fluid": false,
            "customFontSize": true,
            "dropCap": true,
            "fontFamilies": [],
            "fontSizes": [
                {
                    "name": "12px",
                    "size": "0.75rem",
                    "slug": "12"
                },
                {
                    "name": "14px",
                    "size": "0.875rem",
                    "slug": "14"
                },
                {
                    "name": "15px",
                    "size": "0.9375rem",
                    "slug": "15"
                },
                {
                    "name": "16px",
                    "size": "1rem",
                    "slug": "16"
                },
                {
                    "name": "18px",
                    "size": "1.125rem",
                    "slug": "18"
                },
                {
                    "name": "20px",
                    "size": "clamp(1.125rem, 1.125rem + ((1vw - 0.36rem) * 0.2427), 1.25rem)",
                    "slug": "20"
                },
                {
                    "name": "22px",
                    "size": "clamp(1.25rem, 1.25rem + ((1vw - 0.36rem) * 0.2427), 1.375rem)",
                    "slug": "22"
                },
                {
                    "name": "24px",
                    "size": "clamp(1.375rem, 1.375rem + ((1vw - 0.36rem) * 0.2427), 1.5rem)",
                    "slug": "24"
                }
            ],
            "fontStyle": true,
            "fontWeight": true,
            "letterSpacing": true,
            "lineHeight": true,
            "textDecoration": true,
            "textTransform": true
        },
        "blocks": {}
    },
    "styles": {
        "typography": {
            "fontSize": "1rem",
            "lineHeight": "1.5",
            "fontWeight": "400"
        },
        "color": {
            "text": "var(--wp--preset--color--black)",
            "background": "var(--wp--preset--color--white)"
        },
        "spacing": {
            "blockGap": "30px"
        },
        "elements": {
            "link": {
                "typography": {
                    "fontSize": "1rem",
                    "letterSpacing": "",
                    "lineHeight": "1.5",
                    "textDecoration": "none",
                    "textTransform": "uppercase"
                },
                "color": {
                    "text": "#3700ff"
                },
                ":hover": {
                    "color": {
                        "text": "#ff0000"
                    }
                },
                ":focus": {
                    "color": {
                        "text": "#ff0000"
                    },
                    "outline": {
                        "width": "0"
                    }
                }
            },
            "heading": {
                "spacing": {
                    "margin": {
                        "top": "var(--wp--custom--heading--margin--top)",
                        "bottom": "var(--wp--custom--heading--margin--bottom)"
                    },
                    "padding": {
                        "top": "var(--wp--custom--heading--padding--top)"
                    }
                },
                "typography": {
                    "lineHeight": "var(--wp--custom--heading--typography--line-height)",
                    "fontWeight": "var(--wp--custom--heading--typography--font-weight)"
                }
            },
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--custom--heading--typography--font-size--h-1)"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var(--wp--custom--heading--typography--font-size--h-2)"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var(--wp--custom--heading--typography--font-size--h-3)"
                }
            },
            "h4": {
                "typography": {
                    "fontSize": "var(--wp--custom--heading--typography--font-size--h-4)"
                }
            },
            "h5": {
                "typography": {
                    "fontSize": "var(--wp--custom--heading--typography--font-size--h-5)"
                }
            },
            "h6": {
                "typography": {
                    "fontSize": "var(--wp--custom--heading--typography--font-size--h-6)"
                }
            }
        }
    },
    "customTemplates": [],
    "templateParts": []
}
