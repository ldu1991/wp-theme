{
    "version": 2,
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "settings": {
        "appearanceTools": true,
        "useRootPaddingAwareAlignments": true,
        "border": {
            "color": true,
            "radius": true,
            "style": true,
            "width": true
        },
        "color": {
            "background": true,
            "custom": true,
            "customDuotone": false,
            "customGradient": true,
            "defaultGradients": false,
            "defaultPalette": false,
            "duotone": [],
            "gradients": [
                {
                    "slug": "red-linear",
                    "gradient": "linear-gradient(90deg, #E10303 9.49%, #C70202 100%)",
                    "name": "Red Linear"
                }
            ],
            "link": true,
            "palette": [
                {
                    "slug": "white",
                    "color": "#FFFFFF",
                    "name": "White"
                },
                {
                    "slug": "black",
                    "color": "#000000",
                    "name": "Black"
                },
                {
                    "slug": "red",
                    "color": "#ff0000",
                    "name": "Red"
                },
                {
                    "slug": "Blue",
                    "color": "#00209b",
                    "name": "Blue"
                }
            ],
            "text": true
        },
        "custom": {
            "prefix": "DEMO"
        },
        "layout": {
            "contentSize": "min(100% - clamp(1.875rem, 1.875rem + ((1vw - 0.3rem) * 33.2031), 12.5rem), 800px)",
            "wideSize": "min(100% - clamp(1.875rem, 1.875rem + ((1vw - 0.3rem) * 33.2031), 12.5rem), 1200px)"
        },
        "spacing": {
            "blockGap": true,
            "margin": true,
            "padding": true,
            "units": [
                "%",
                "px",
                "em",
                "rem",
                "vh",
                "vw"
            ]
        },
        "typography": {
            "fluid": false,
            "customFontSize": true,
            "dropCap": true,
            "fontFamilies": [
                {
                    "fontFamily": "'Roboto Condensed', sans-serif",
                    "name": "Roboto Condensed",
                    "slug": "roboto-condensed",
                    "google": "family=Roboto+Condensed:ital,wght@0,300;0,400;0,700;1,300;1,400;1,700"
                },
                {
                    "fontFamily": "'Kanit', sans-serif",
                    "name": "Kanit",
                    "slug": "kanit",
                    "google": "family=Kanit:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900"
                }
            ],
            "fontSizes": [
                {
                    "name": "12px",
                    "size": "0.75rem",
                    "slug": "12"
                },
                {
                    "name": "14px",
                    "size": "0.875rem",
                    "slug": "14"
                },
                {
                    "name": "15px",
                    "size": "0.9375rem",
                    "slug": "15"
                },
                {
                    "name": "16px",
                    "size": "1rem",
                    "slug": "16"
                },
                {
                    "name": "18px",
                    "size": "1.125rem",
                    "slug": "18"
                },
                {
                    "name": "20px",
                    "size": "clamp(1.125rem, 1.125rem + ((1vw - 0.36rem) * 0.2427), 1.25rem)",
                    "slug": "20"
                },
                {
                    "name": "22px",
                    "size": "clamp(1.25rem, 1.25rem + ((1vw - 0.36rem) * 0.2427), 1.375rem)",
                    "slug": "22"
                },
                {
                    "name": "24px",
                    "size": "clamp(1.375rem, 1.375rem + ((1vw - 0.36rem) * 0.2427), 1.5rem)",
                    "slug": "24"
                },
                {
                    "name": "26px",
                    "size": "clamp(1.5rem, 1.5rem + ((1vw - 0.36rem) * 0.2427), 1.625rem)",
                    "slug": "26"
                }
            ],
            "fontStyle": true,
            "fontWeight": true,
            "letterSpacing": true,
            "lineHeight": true,
            "textDecoration": true,
            "textTransform": true
        },
        "blocks": {}
    },
    "styles": {
        "blocks": {
            "core/paragraph": {
                "spacing": {
                    "margin": {
                        "top": "0",
                        "bottom": "20px"
                    }
                }
            }
        },
        "spacing": {
            "blockGap": "30px",
            "margin": {
                "top": "0",
                "right": "0",
                "bottom": "0",
                "left": "0"
            }
        },
        "color": {
            "background": "var(--wp--preset--color--white)",
            "text": "var(--wp--preset--color--black)"
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--roboto-condensed)",
            "fontSize": "1rem",
            "lineHeight": "1.5",
            "fontWeight": "400"
        },
        "elements": {
            "link": {
                "typography": {
                    "fontStyle": "",
                    "fontFamily": "",
                    "fontWeight": "",
                    "fontSize": "1rem",
                    "letterSpacing": "",
                    "textTransform": "uppercase",
                    "lineHeight": "1.5",
                    "textDecoration": "none"
                },
                "color": {
                    "text": "#3700ff"
                },
                ":hover": {
                    "color": {
                        "text": "#ff0000"
                    }
                },
                ":focus": {
                    "outline": {
                        "width": "0"
                    }
                }
            },
            "heading": {
                "spacing": {
                    "margin": {
                        "top": "0",
                        "bottom": "30px"
                    },
                    "padding": {
                        "top": "20px"
                    }
                },
                "typography": {
                    "fontStyle": "",
                    "fontFamily": "",
                    "fontWeight": "700",
                    "letterSpacing": "",
                    "textTransform": "",
                    "lineHeight": "1.5"
                }
            },
            "h1": {
                "typography": {
                    "fontSize": "clamp(2.625rem, 2.625rem + ((1vw - 0.36rem) * 1.2136), 3.25rem)"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "clamp(2.375rem, 2.375rem + ((1vw - 0.36rem) * 0.7282), 2.75rem)"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "clamp(2rem, 2rem + ((1vw - 0.36rem) * 0.7282), 2.375rem)"
                }
            },
            "h4": {
                "typography": {
                    "fontSize": "clamp(1.75rem, 1.75rem + ((1vw - 0.36rem) * 0.4854), 2rem)"
                }
            },
            "h5": {
                "typography": {
                    "fontSize": "clamp(1.25rem, 1.25rem + ((1vw - 0.36rem) * 0.7282), 1.625rem)"
                }
            },
            "h6": {
                "typography": {
                    "fontSize": "clamp(1.125rem, 1.125rem + ((1vw - 0.36rem) * 0.4854), 1.375rem)"
                }
            }
        }
    },
    "customTemplates": [],
    "templateParts": []
}
